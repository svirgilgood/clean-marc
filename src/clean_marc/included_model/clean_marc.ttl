cmo:Agent
  a owl:Class ;
  skos:prefLabel "Agent" ;
  .

cmo:Category
  a owl:Class ;
  skos:prefLabel "Category" ;
  .

cmo:MarcField
  a owl:Class ;
  skos:prefLabel "Marc Field" ;
  skos:definition "A collection of the parts that make up a marc field" ;
  .

cmo:ManuscriptResource
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      cmo:Agent
      [
        a owl:Restriction ;
        owl:onProperty cmo:hasMarcField ;
        owl:someValuesFrom cmo:ManuscriptField ;
      ]
    ) ;
  ] ;
  skos:prefLabel "Manuscript Resource" ;
  .

cmo:ManuscriptField
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
      cmo:MarcField
      [
        a owl:Restriction ;
        owl:onProperty cmo:hasPart ;
        owl:someValuesFrom [
          a owl:Class ;
          owl:intersectionOf (
            cmo:SubField
            [
              a owl:Restriction ;
              owl:onProperty cmo:hasCategory ;
              owl:hasValue cmt:_SFCat_Part ;
            ]
          )
        ] ;
      ]
      [
        a owl:Restriction ;
        owl:onProperty cmo:hasPart ;
        owl:someValuesFrom [
          a owl:Class ;
          owl:intersectionOf (
            cmo:SubField
            [
              a owl:Class ;
              owl:unionOf (
                [
                  a owl:Restriction ;
                  owl:onProperty cmo:hasCategory ;
                  owl:hasValue cmt:_SFCat_Subtitle ;
                ]
                [
                  a owl:Restriction ;
                  owl:onProperty cmo:hasCategory ;
                  owl:hasValue cmt:_SFCat_Language
                ]
                [
                  a owl:Restriction ;
                  owl:onProperty cmo:hasCategory ;
                  owl:hasValue cmt:_SFCat_SubordinateUnit ;
                ]
              )
            ]
          )
        ]
      ]
    ) ;
  ] ;
  skos:prefLabel "Manuscript Reference" ;
  .

cmo:MarcField
  a owl:Class ;
  skos:prefLabel "Marc Field" ;
  .

cmo:NameType
  rdfs:subClassOf cmo:Category ;
  skos:prefLabel "Name Type" ;
  .

cmo:Organization
